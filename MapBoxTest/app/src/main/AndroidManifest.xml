<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.bignerdranch.mapboxtest">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:name="androidx.multidex.MultiDexApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MapBoxTest">
        <activity android:name=".ExistingPlaylistTabActivity"></activity>
        <activity android:name=".ExistingPlaylistActivity" />
        <activity android:name=".JourneyActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".GenreScreenActivity" />
        <activity android:name=".SongListActivity" />
        <activity android:name=".SingleFragmentActivity" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>